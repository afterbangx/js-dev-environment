{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "//": [
    "to override rules add them to the rules object below",
    "0 - off, 1 - warning, 2 - Error"
  ],
  "rules": {
    "no-console": 1
  }
}
